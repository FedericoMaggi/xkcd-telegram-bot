{
  "name": "xkcd-telegram-bot",
  "version": "1.1.0",
  "description": "An unofficial node.js based telegram bot for to share the awesomeness of xkcd.com ",
  "main": "bot.js",
  "scripts": {
    "container": "node bot.js",
    "boot": "forever start --uid \"xkcdbot\" -a bot.js",
    "stop": "forever stop \"xkcdbot\"",
