  "bugs": {
    "url": "https://github.com/fredmaggiowski/xkcd-telegram-bot/issues"
  },
  "readme": "Nothing to read for",
  "homepage": "https://github.com/fredmaggiowski/xkcd-telegram-bot#README",
  "dependencies": {
    "bunyan": "^1.8.12",
    "node-telegram-bot-api": "^0.30.0",
    "redis": "^2.3.1",
    "request": "^2.88.0"
  }
}
